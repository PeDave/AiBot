{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "BitgetApi.TradingEngine.HostedServices.N8NHostedService": "Debug",
      "BitgetApi.TradingEngine.HostedServices.StrategyAnalysisService": "Debug"
    }
  },
  "N8N": {
    "Enabled": true,
    "StartupDelaySeconds": 20,
    "MaxWaitSeconds": 35,
    "MaxRetries": 5
  },
  "Trading": {
    "EnableAutoTrading": false,
    "UseN8NForDecisions": true,
    "MinStrategyAgreement": 1,
    "MinConfidence": 50.0
  },
  "StrategyAnalysis": {
    "InitialDelaySeconds": 3
  }
}
